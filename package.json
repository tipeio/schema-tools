{
  "name": "@tipe/schema-tools",
  "version": "0.0.6",
  "main": "dist/index.js",
  "license": "MIT",
  "scripts": {
    "build": "babel src --out-dir dist",
    "start": "yarn rm && yarn build && node dist/start.js",
    "rm": "rm -rf dist",
    "test": "jest",
    "restart": "yarn notify && yarn start",
    "notify": "osascript -e 'display notification \"gql app restarted\" with title \"Nodemon\"'",
    "dev": "nodemon --exec yarn restart",
    "preversion": "yarn test",
    "postversion": "git push origin master && git push --tags && yarn build",
    "inspect": "yarn rm && yarn build && node inspect dist/server.js",
    "lint": "eslint --ext .js, src",
    "lint-autofix": "yarn lint --fix",
    "eslint-check": "eslint --print-config .eslintrc.js | eslint-config-prettier-check",
    "precommit": "lint-staged && yarn test",
    "deploy": "yarn build && yarn publish"
  },
  "lint-staged": {
    "linters": {
      "*.js": [
        "eslint --fix",
        "git add"
      ]
    }
  },
  "dependencies": {
    "camel-case": "^3.0.0",
    "chance": "^1.0.16",
    "graphql": "^0.13.2",
    "graphql-codegen-core": "^0.10.3",
    "graphql-parse-resolve-info": "^4.0.0-rc.4",
    "graphql-tools": "^3.0.4",
    "lodash": "^4.17.10",
    "moment": "^2.22.2",
    "pascal-case": "^2.0.1",
    "pluralize": "^7.0.0",
    "validator": "^10.4.0"
  },
  "devDependencies": {
    "@tipe/eslint-config-tipe": "^1.0.1",
    "apollo-server": "^2.0.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-jest": "^23.4.2",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-decorators": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.5",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "husky": "^0.14.3",
    "jest": "^23.4.2",
    "lint-staged": "^7.2.0",
    "nodemon": "^1.18.3",
    "prettier": "^1.14.0"
  },
  "jest": {
    "verbose": true,
    "testURL": "http://localhost/",
    "testEnvironment": "node"
  }
}
